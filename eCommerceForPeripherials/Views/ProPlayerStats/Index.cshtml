@model IEnumerable<HltvParser.Player>

@{
    ViewData["Title"] = "Index";
}

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Maps)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Rounds)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.KD_Diff)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.KD)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Rating)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Maps)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Rounds)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.KD_Diff)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.KD)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Rating)
            </td>
        </tr>
}
    </tbody>
</table>
