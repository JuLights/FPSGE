@model IEnumerable<HltvParser.PlayersGear>

@{
    ViewData["Title"] = "PlayersGear";
}

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Team)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Role)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Mouse)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.KeyBoard)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Headset)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MouseHz)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DPI)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Sens)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.eDPI)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Monitor)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MonitorHz)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GPU)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Resolution)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AspectRatio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ScalingMode)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MousePad)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Team)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Role)
            </td>
            <td>
                <a href="@item.MouseLink">@Html.DisplayFor(modelItem => item.Mouse)</a>
            </td>
            <td>
                <a href="@item.KeyBoardLink">@Html.DisplayFor(modelItem => item.KeyBoard)</a>
            </td>
            <td>
                <a href="@item.HeadsetLink">@Html.DisplayFor(modelItem => item.Headset)</a>
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MouseHz)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DPI)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Sens)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.eDPI)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Monitor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MonitorHz)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GPU)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Resolution)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AspectRatio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ScalingMode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MousePad)
            </td>
        </tr>
}
    </tbody>
</table>
