@model IEnumerable<eCommerceForPeripherials.Models.Item>
@{
    ViewData["Title"] = "FPS.GE";
}

<!-- inner page banner -->
<div id="inner_banner" class="section inner_banner_section">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="full">
                    <div class="title-holder">
                        <div class="title-holder-cell text-left">
                            <h1 class="page-title">Shop Page</h1>
                            <ol class="breadcrumb">
                                <li><a asp-controller="Home" asp-action="Index">Home</a></li>
                                <li class="active">Shop</li>
                            </ol>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- end inner page banner -->
<!-- section -->
<div class="section padding_layout_1 product_list_main" style="background-color:black;">
    <div class="container">
        <div class="row">
            <div class="col-md-9">
                <div class="col-md-3">
                    <div class="side_bar">
                        <div class="side_bar_blog">
                            <h4 style="color:white;">SEARCH</h4>
                            <div class="side_bar_search">
                                <div class="input-group stylish-input-group">
                                    <input class="form-control" placeholder="Search" type="text">
                                    <span class="input-group-addon">
                                        <button type="submit"><i class="fa fa-search" aria-hidden="true"></i></button>
                                    </span>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="row">
                    @{
                        if (Model != null)
                        {
                            foreach (var item in Model)
                            {
                                <div class="col-md-4 col-sm-6 col-xs-12 margin_bottom_30_all">
                                    <div class="product_list">
                                        <div class="product_img"> <img class="img-responsive" src="@item.ItemImageUrl" alt=""> </div>
                                        <div class="product_detail_btm">
                                            <div class="center">
                                                <h4><a asp-controller="Shop" asp-action="Details" asp-route-id="@item.Id">@item.Name</a></h4>
                                            </div>
                                            <div class="starratin">
                                                <div class="center"> <i class="fa fa-star" aria-hidden="true"></i> <i class="fa fa-star" aria-hidden="true"></i> <i class="fa fa-star" aria-hidden="true"></i> <i class="fa fa-star" aria-hidden="true"></i> <i class="fa fa-star-o" aria-hidden="true"></i> </div>
                                            </div>
                                            <div class="product_price">
                                                <p><span class="old_price">200.00₾</span> – <span class="new_price" style="color:#9999;">@item.Price₾</span></p>
                                            </div>
                                            <div class="descr" style="padding:5px;">
                                                <p>@item.Description</p>
                                                @if (User.IsInRole("Admin"))
                                                {
                                                    <form id="deleteItem" method="post" asp-action="DeleteItem" asp-controller="Admin" asp-route-id="@item.Id"><a class="btn btn-danger" href="javascript:document.getElementById('deleteItem').submit()">delete</a></form>
                                                }

                                            </div>

                                        </div>
                                    </div>
                                </div>
                            }
                        }

                    }




